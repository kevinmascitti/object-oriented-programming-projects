package sports;

import java.util.LinkedList;
import java.util.List;

public class Prodotto {

	String name;
	Activity attivita;
	Category categoria;
	List<Review> recensioni = new LinkedList<>();

	public Prodotto(String name, Activity activity, Category category) {
		this.name=name;
		this.attivita=activity;
		this.categoria=category;
	}

	public String getName() {
		return name;
	}
}
