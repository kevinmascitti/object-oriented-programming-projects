package sports;

public class Review {

	Prodotto prodotto;
	String username;
	String comment;
	int numStars;

	public Review(Prodotto prodotto, String userName, String comment, int numStars) {
		this.prodotto=prodotto;
		this.username=userName;
		this.comment=comment;
		this.numStars=numStars;
	}
	
	public int getStars() {
		return numStars;
	}

}
